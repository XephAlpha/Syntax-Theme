  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#404040"
      }
    },
    {
      "name": "Red",
      "scope": [
        "string",
        "constant",
        "support.constant",
        "meta.property-value",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#e476b2"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "entity.other.attribute-name",
        "variable"
      ],
      "settings": {
        "foreground": "#96b2e4"
      }
    },
    {
      "name": "Green",
      "scope": [],
      "settings": {
        "foreground": "#46b482"
      }
    },
    {
      "name": "Cyan",
      "scope": [
        "punctuation.definition.tag",
        "entity.name.tag",
        "storage",
        "keyword",
        "meta.object-literal.key",
        "meta.object-literal.key string",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#76e4e4"
      }
    },
    {
      "name": "Magenta",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class",
        "support.class",
        "support.variable"
      ],
      "settings": {
        "foreground": "#c296e4"
      }
    },
    {
      "name": "Yellow",
      "scope": [],
      "settings": {
        "foreground": "#a2e476"
      }
    },
    {
      "name": "Gray",
      "scope": [],
      "settings": {
        "foreground": "#505050"
      }
    },
    {
      "name": "Black",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#707070"
      }
    }
  ]
}
