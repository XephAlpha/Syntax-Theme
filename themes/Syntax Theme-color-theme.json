    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#404040"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "punctuation.definition.tag",
                "entity.name.tag",
                "variable.language",
                "constant.language",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.class",
                "keyword.other.important",
                "storage"

            ],
            "settings": {
                "foreground": "#6688CC"
            }
        },
        {
            "name": "Red",
            "scope": [
                "string",
                "support.constant"
            ],
            "settings": {
                "foreground": "#F07178"
            }
        },
        {
            "name": "Green",
            "scope": [
                "support.type",
                "support.class",
                "support.variable",
                "support.function",
                "markup.underline.link",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#AEDD7E"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "entity"
            ],
            "settings": {
                "foreground": "#FFCB6B"
            }
        },
        {
            "name": "Default",
            "scope": [
                "punctuation.section.embedded",
                "meta.object-literal.key punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#C0C0C0"
            }
        }
    ]
}
