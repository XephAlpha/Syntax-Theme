    "tokenColors": [
        {
            "name": "Comment",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#C0C0C0"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "punctuation.definition.tag",
                "entity.name.tag",
                "variable.language",
                "keyword.control",
                "keyword.operator.new",
                "keyword.operator.expression",
                "keyword.operator.class",
                "keyword.other.important",
                "storage"
            ],
            "settings": {
                "foreground": "#0451A5"
            }
        },
        {
            "name": "Red",
            "scope": [
                "string",
                "support.constant"
            ],
            "settings": {
                "foreground": "#A31515"
            }
        },
        {
            "name": "Green",
            "scope": [
                "support.type",
                "support.class",
                "support.variable",
                "support.function",
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#008000"
            }
        },
        {
            "name": "Default",
            "scope": [
                "punctuation.section.embedded",
                "meta.object-literal.key punctuation.separator.key-value"
            ],
            "settings": {
                "foreground": "#404040"
            }
        },
        {
            "name": "Yellow",
            "scope": [
                "entity",
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#BF8B00"
            }
        },
        {
            "name": "Orange",
            "scope": [
                "constant.language",
                "constant.numeric",
                "constant.other"
            ],
            "settings": {
                "foreground": "#E67E00"
            }
        }
    ]
}
