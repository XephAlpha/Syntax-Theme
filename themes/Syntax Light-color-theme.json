  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "name": "Red",
      "scope": [
        "string",
        "constant",
        "support.constant",
        "meta.property-value",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#b44682"
      }
    },
    {
      "name": "Blue",
      "scope": [
        "entity.other.attribute-name",
        "variable"
      ],
      "settings": {
        "foreground": "#4682b4"
      }
    },
    {
      "name": "Green",
      "scope": [],
      "settings": {
        "foreground": "#46b482"
      }
    },
    {
      "name": "Cyan",
      "scope": [
        "punctuation.definition.tag",
        "entity.name.tag",
        "storage",
        "keyword",
        "meta.object-literal.key",
        "meta.object-literal.key string",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#46b4b4"
      }
    },
    {
      "name": "Magenta",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.type",
        "support.type",
        "entity.other.inherited-class",
        "support.class",
        "support.variable"
      ],
      "settings": {
        "foreground": "#8246b4"
      }
    },
    {
      "name": "Yellow",
      "scope": [],
      "settings": {
        "foreground": "#82b446"
      }
    },
    {
      "name": "Gray",
      "scope": [],
      "settings": {
        "foreground": "#505050"
      }
    },
    {
      "name": "Black",
      "scope": [
        "keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#707070"
      }
    }
  ]
